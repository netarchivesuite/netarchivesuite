            </newObject>
            <newObject name="HTTP" class="org.archive.crawler.fetcher.FetchHTTP">
                <boolean name="enabled">true</boolean>
                <newObject name="HTTP#decide-rules" class="org.archive.crawler.deciderules.DecideRuleSequence">
          		<map name="rules">
